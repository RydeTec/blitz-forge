Using "RC_Core.rcm"
Using "ProcessGlobals.rcm"

Function Main()

	If FileType("League1.DATA") <> 1
		FH = WriteFile("League1.DATA")
		CloseFile(FH)
	EndIf
	If FileType("League2.DATA") <> 1
		FH = WriteFile("League2.DATA")
		CloseFile(FH)
	EndIf
	If FileType("League3.DATA") <> 1
		FH = WriteFile("League3.DATA")
		CloseFile(FH)
	EndIf
	DeleteFile("League1_SORT.SEM")
	DeleteFile("League2_SORT.SEM")
	DeleteFile("League3_SORT.SEM")
End Function